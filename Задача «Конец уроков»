a = int(input("Введите число "))
a = a*45 + (a//2)*5 + ((a+1)//2-1)*15
print(a//60 + 9, a%60)


a = int(input())
a = a*45 + (a//2)*5 + ((a+1)//2-1)*15
print(a//60 + 9, a%60)


a=int(input())
if a%2==0:
    b=a*45+(a//2)*5+(a//2-1)*15
else:
    b=a*45+(a//2)*5+(a//2)*15
print(str(9+b//60))
print(str(b%60))


